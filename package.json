{
  "name": "@bingo347/eslint-config",
  "version": "1.7.0",
  "description": "ESLint rules preset",
  "main": "index.js",
  "files": [
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "check": "ts-node check-missing-rules.ts",
    "build": "rollup --configPlugin rollup-plugin-ts -c rollup.config.ts",
