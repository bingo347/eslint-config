  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bingo347/eslint-config/issues"
  },
  "homepage": "https://github.com/bingo347/eslint-config#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/eslint": "^8.56.1",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "rollup": "^4.9.2",
    "rollup-plugin-ts": "^3.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": ">=7",
    "eslint-plugin-eslint-comments": "^3.2.0",
